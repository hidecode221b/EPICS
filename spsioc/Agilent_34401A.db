record(bo, "$(sys):$(sec):$(dev):setRemote")
{
  field(DESC, "set rem")
  field(DTYP, "stream")
  field(OUT, "@Agilent_34401A.proto setRemote $(PORT)")
  field(ZNAM, "Local")
  field(ONAM, "Remote")
  field(PINI, "YES")
  field(VAL, "1")
}

record(ai, "$(sys):$(sec):$(dev):getOutVolt")
{
  field(DTYP, "stream")
  field(INP, "@Agilent_34401A.proto getOutVolt $(PORT)")
  field(PREC, "10")
  field(SCAN, "2 second")
}

