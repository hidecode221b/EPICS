record(bo, "setRemote")
{
  field(DESC, "set rem")
  field(DTYP, "stream")
  field(OUT, "@Agilent_volt.proto setRemote $(PORT)")
  field(ZNAM, "Local")
  field(ONAM, "Remote")
  field(PINI, "YES")
  field(VAL, "1")
}

record(ai, "getOutVolt")
{
  field(DTYP, "stream")
  field(INP, "@Agilent_volt.proto getOutVolt $(PORT)")
  field(PREC, "10")
  field(SCAN, "2 second")
}

